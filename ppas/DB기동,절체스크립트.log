ps -ef | grep edb-postgres

<중지>
pg_ctl -mf stop
/usr/bin/pg_ctl stop -m fast -D /var/lib/pgsql/data 

<기동>
pg_ctl start -w -D /data/AKOS
/usr/bin/pg_ctl start -w -D /var/lib/pgsql/data  -o "-p 5432" 
sudo -U enterprisedb /postgres/9.6AS/bin/pg_ctl start -w -D /data/AKOS
 
<재기동>
pg_ctl restart -w -D /data/AKOS
/usr/bin/pg_ctl restart -w -D /var/lib/pgsql/data 


